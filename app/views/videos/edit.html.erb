<h1>Edit video</h1>

<%= render 'err' %>

<%= form_for @video do |f| %>

<p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
</p>

<p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
</p>

<p>
    <%= f.submit %>
</p>

<% end %>

<%= link_to 'Back', videos_path %>
